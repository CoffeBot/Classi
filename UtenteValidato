package immobiliare;

public class UtenteValidato extends Utente
{
    package boolean isValidato;
    
    public UtenteValidato(boolean isValidato)
    {
        super(String nome, String cognome, String email, int telefono, int userID);
        this.isValidato=isValidato;
    }
    
    /*il valore di isValidato pu√≤ essere modificato dal metodo registrazione*/
}
