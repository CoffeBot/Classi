package immobiliare;

import java.util.Vector;
import java.util.ArrayList;

public class AccumulatoreAnnunci {

	public Annuncio annunci;
	
	public Vector  myZona;
	public Vector  myAnnuncio;
	
	public static ArrayList<Annuncio> listaAnnuncio=new ArrayList<Annuncio>();//lista degli annunci
	
	
	public static void Inserisci(Annuncio p) //aggiungo annuncio alla lista
	{ 
		listaAnnunci.add(p);
	}
	
	public static void Rimuovi(Annuncio p) //rimuovo la prenotazione dalla lista
	{ 
		AccumulatoreAnnunci.listaAnnunci.remove(p);
	}
	
	public void Revisiona(Annuncio p)
	{
		AccumulatoreAnnunci.listaAnnunci.edit(p);
	}
  
}
