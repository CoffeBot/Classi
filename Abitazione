import java.util.Vector;

public class Abitazione {

	public int nVani;
	public String via;
	public int mq;
	public int piano;
	public int identificatore;
	public int zonaC; //va a rappresentare il cap della zona

	public Abitazione(int nVani, String via, int mq, int piano, int identificatore,
		int zonaC)
	{
		this.nVani=nVani;
		this.via=via;
		this.mq=mq;
		this.piano=piano;
		this.identificatore=identificatore;
		this.zonaC=zonaC;
	}

}
