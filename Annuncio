package immobiliare;
import java.util.Vector;

public class Annuncio{

  private Abitazione caratteristica;
  public int prezzo;
  public String contatti;
  public Varie[] cose; //conterr√† immagini relative all'abitazione
  public int[] serviziVicini= new array[4];

  public Vector  myAccumulatoreAnnunci;
  public Vector  myMessaggio;
  public Vector  myAbitazione;

  public Annuncio ( Abitazione caratteristica, int prezzo, String contatti, Varie, 
        Vector  myAccumulatoreAnnunci, Vector  myMessaggio, Vector  myAbitazione)
  {
    this.caratteristica=caratteristica;
    this.prezzo=prezzo;
    this.contatti=contatti;
    this.Varie=varie;
    this.myAccumulatoreAnnunci=myAccumulatoreAnnunci;
    this.myMessaggio=myMessaggio;
    this.myAbitazione=myAbitazione;
    
  }
  public void ContattaAgente(Messaggio u) 
  {
    Annunci.ContattaAgente.send(u);
  }

}
